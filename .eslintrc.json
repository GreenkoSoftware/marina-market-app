{
    "extends": [
        "next/core-web-vitals",
        "eslint:recommended"
      ],
      "overrides": [],
      "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
      },
      "plugins": ["react"],
      "rules": {
        "no-mixed-spaces-and-tabs": 0, // disable rule,
        "no-undef": "warn",
        "object-curly-spacing": ["error", "always"],
        "react/prop-types": ["off"],
        "react-hooks/rules-of-hooks": "off",
        "react-hooks/exhaustive-deps": "off",
        "no-tabs": 0
      }, 
      "env": {
        "browser": true,
        "es2021": true
      }
}